{
  "name": "buzzmaster",
  "version": "1.0.0-beta.3",
  "description": "A buzzer application for quiz shows",
  "productName": "BuzzMaster",
  "author": "marvin-wtt <31454580+marvin-wtt@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/marvin-wtt/BuzzMaster"
  },
  "keywords": [
    "playstation",
    "buzz",
    "buzzer",
    "quiz"
  ],
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev -m electron",
    "build": "quasar build -m electron",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.11",
    "electron-log": "^5.1.4",
    "electron-updater": "^6.1.8",
    "events": "^3.3.0",
    "pinia": "^2.1.7",
    "quasar": "^2.16.4",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@pinia/testing": "^0.1.3",
    "@quasar/app-vite": "^2.0.0-beta.14",
    "@quasar/cli": "^2.4.1",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.12",
    "@types/w3c-web-hid": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitest/ui": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "electron": "^30.0.7",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "lodash-es": "^4.17.21",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.2.5",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.4.5",
    "vite-plugin-checker": "^0.6.4",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": "^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
